# MongoDB Connection
MONGODB_URI=mongodb+srv://username:password@cluster0.example.mongodb.net/
MONGODB_DB_NAME=sdof_knowledge
MONGODB_COLLECTION=knowledge_entries

# Embedding Service Configuration
# Choose one of the embedding services below and provide the appropriate API key

# OpenAI (Default)
EMBEDDING_SERVICE=openai
OPENAI_API_KEY=your_openai_api_key
EMBEDDING_MODEL=text-embedding-3-large

# Claude (Alternative)
# EMBEDDING_SERVICE=claude
# CLAUDE_API_KEY=your_claude_api_key

# Google Gemini (Alternative)
# EMBEDDING_SERVICE=gemini
# GEMINI_API_KEY=your_gemini_api_key

# Deepseek (Alternative)
# EMBEDDING_SERVICE=deepseek
# DEEPSEEK_API_KEY=your_deepseek_api_key

# Server Configuration
PORT=3000